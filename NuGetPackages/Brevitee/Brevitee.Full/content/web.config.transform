<configuration>
	<appSettings>
		<add key="ProxySearchPattern" value="*Servers.dll" />		
		<add key="ApplicationName" value="SetAppNameInConfig" />
		<add key="Roles" value="Admin, Users" />
		<add key="EdwardServerUrl" value="http://devplatform.klgates.com" />
		<add key="EdwServerUrl" value="http://devplatform.klgates.com" />
	</appSettings>
	<system.web>		
		<httpModules>
			<add name="EdwardRoleProvider" type="Brevitee.Platform.EdwardRoleProvider, Brevitee.Platform.Clients, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
		</httpModules>
		<roleManager enabled="true" defaultProvider="EdwardRoleProvider">
		  <providers>
			<clear/>
			<add name="EdwardRoleProvider" type="Brevitee.Platform.EdwardRoleProvider, Brevitee.Platform.Clients, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
		  </providers>
		</roleManager>
		<pages>
			<namespaces>
				<add namespace="Brevitee" />
				<add namespace="Brevitee.Configuration" />
				<add namespace="Brevitee.Data" />
				<add namespace="Brevitee.Data.MsSql" />
				<add namespace="Brevitee.Dust" />
				<add namespace="Brevitee.ServiceProxy" />
				<add namespace="Brevitee.Html" />
				<add namespace="Brevitee.Incubation" />
				<add namespace="Brevitee.Logging" />
			</namespaces>
		</pages>
	</system.web>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true">
		  <add name="EdwardRoleProvider" type="Brevitee.Platform.EdwardRoleProvider, Brevitee.Platform.Clients, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
		</modules>
	</system.webServer>
</configuration>